PROGRAM fibonacci
BEGIN

	STRING input := "Please input an integer number: ";
	STRING space := " ";
	STRING eol := "\n";

	FUNCTION INT F (INT n)
	BEGIN

		IF (n > 2)
		     RETURN F(n-1)+F(n-2);
		ELSIF (n = 0)   --This is a comment			
			RETURN 0;
		ELSIF (n = 1)
			RETURN 1;
		ELSIF (n = 2)
			RETURN 1;
	     ENDIF
	END
	
	
	FUNCTION VOID main ()
	BEGIN
		INT i, end, result;
		WRITE(input);
		READ(end);

	i := 0;
	DO
		result := F(i);
		WRITE (i,space);
		WRITE (result,eol);
		i := i + 1;	
	WHILE (i != end);

	END

END	
